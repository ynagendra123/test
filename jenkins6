pipeline {
	agent any 
	  stages { 
	      stage('build') {
             when { branch "main" }
			  steps { 
			     echo "hellow"
				 echo "welcome"
			        }
				 }
			stage('second') {
			branch "main" 
			when {
				env.BRANCH == 'main'
				 }
							}
				steps { 
							echo "good morning"
				      }
			  }
			}
